\draw [fill=white] (1.5,0.05) circle (0.1);
\node [forcearrow,rotate=180] at (2.2,0) {};
\node at (2.7,0) {$F$};
\draw [|<->|] (-2,-2.2) -- node [below] {$L$} (2,-2.2);
\node at (0,1) {$F \Delta t = \Delta (mv_{avg})$};
\node at (0,-1) {$2L = (v_{avg}/3) \Delta t$};
\end{tikzpicture}
\end{minipage}
\hfill
\begin{minipage}{57mm}
\small
\begin{align*}
       (F)(6L/v_{avg}) &= 2mv_{avg}                      \\
                    FL &= \tfrac{1}{3}mv_{avg}^2         \\
                    PV &= \tfrac{1}{3}mv_{avg}^2         \\
                    PV &= \tfrac{2}{3}(\half mv_{avg}^2) \\
                    PV &= \tfrac{2}{3}(KE_{avg})         \\
                       &\Downarrow                       \\
\tfrac{2}{3}(KE_{avg}) &= nRT                            \\
              KE_{avg} &= \tfrac{3}{2}nRT                \\
              KE_{avg} &= \tfrac{3}{2}NkT
\end{align*}
\end{minipage}
