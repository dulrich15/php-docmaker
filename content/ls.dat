\begin{tikzpicture}
% This shades the projection
\fill [black!10] (\ang:1) -- +(90+\ang:\vel) |- ({cos(\ang)},\bot) -- cycle;
% This is the circular motion
\draw [help lines] (0,0) circle (\rad);
\draw [help lines] (0,0) -- (\ang:\rad);
\draw [->] (\ang:\rad) -- +(90+\ang:\vel);
\fill (\ang:\rad) circle (0.05);
% This is the projected motion
\draw [|-|,help lines] (-\rad,\bot) -- (\rad,\bot);
\draw [->] ({cos(\ang)},\bot) -- +({\vel*cos(90+\ang)},0);
\fill ({cos(\ang)},\bot) circle (0.05);
% These are the equations
\node [yshift=2cm] 
{
\begin{minipage}{3cm}
\centering $v = \dfrac{2 \pi r}{T} = A \omega$
\end{minipage}
};
\node [eqn,yshift=-3cm] 
{
\begin{minipage}{3cm}
\centering $v = A \omega \sin (\omega t)$ \\[1mm] $v_{max} = A \omega$
\end{minipage}
};
