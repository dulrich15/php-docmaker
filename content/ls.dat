\def\vel{0.8}
\def\acc{0.8*0.8}
\def\bot{-1.5\rad}
%%%%%%%% ucm-to-shm-position %%%%%%%%
\begin{tikzpicture}
% This shades the projection
\fill [black!10] (\ang:1) -- (0,0) |- ({cos(\ang)},\bot) -- cycle;
% This is the circular motion
\draw [help lines] (0,0) circle (\rad);
\draw [help lines] (0,0) -- (0:\rad);
\draw [->] (0,0) -- (\ang:\rad);
\fill (\ang:\rad) circle (0.05);
% This is the projected motion
\draw [|-|,help lines] (-\rad,\bot) -- (\rad,\bot);
\draw [->] (0,\bot) -- +({cos(\ang)},0);
\fill ({cos(\ang)},\bot) circle (0.05);
% These are the equations
\node [yshift=2cm] 
{
\begin{minipage}{3cm}
\centering $r = A$ \\[1mm] $\theta = \omega t$
\end{minipage}
};
\node [eqn,yshift=-3cm]
{
\begin{minipage}{3cm}
\centering $x = A \cos (\omega t)$ \\[1mm] $x_{max} = A$
\end{minipage}
};
\end{tikzpicture}
