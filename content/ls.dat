\draw [->,dotted] (2,0) -- node (y) [right] {$a_y = 3\vhat{y}$} +(0,3);
\node at (0.5,0.3) {$\theta$};
\node (z) [left] at (4,-1.5) {Components};
\draw [->] (z) to (x);
\draw [->] (z) to (y);
\end{scope}

\node (r) [minimum height=50mm,minimum width=50mm] at (3,0) {};
\node at (r) 
{
\begin{minipage}{48mm}
\centering
How to calculate components: \\[5mm]
$a_x = a \cos \theta$ \\
$a_y = a \sin \theta$ \\[5mm]
Measure the angle $\theta$ relative to the $\vhat{x}$ vector. \\[5mm]
$\vmag{a} = a = \sqrt{a_x^2 + a_y^2}$ \\
$\vang{a} = \theta = \tan^{-1} (a_y / a_x)$ \\[2mm]
\tiny (If $a_x < 0$, add 180$\dg$ to this answer)
\end{minipage}
};

\end{tikzpicture}
